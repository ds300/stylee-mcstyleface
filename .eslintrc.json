{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-undef": 2,
        "comma-dangle": [2, "only-multiline"]
    },
    "env": {
      "node": true
    }
}
